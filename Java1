package com.shaik.testprograms;

import java.util.HashMap;
import java.util.Map;

public class HashMapDemo {
	public static void main(String[] args) {
		
		Map<String, Integer> map=new HashMap<>();
		
		map.put("aaa", 1);
		map.put("bbb", 1);
		map.put("ccc", 1);
		map.put("ddd", 1);
		
		System.out.println("Hello Git Hub...");
		
	}
}
